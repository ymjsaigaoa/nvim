        "typescript",
        "html",
        "css",
        "json"
    ],
    "html.enable": true,
    "html.format.enable": true,
    "css.lint.hexColorLength": "warning",
    "css.lint.duplicateProperties": "warning",
    "explorer.file.showHiddenFiles": true,
    "explorer.file.column.indent.indentLine": true,
    "explorer.file.column.git.showIgnored": true,
    "explorer.keyMappingMode": "none",
    "explorer.buffer.showHiddenBuffers": false,
    "explorer.keyMappings": {
        "k": "nodePrev",
        "j": "nodeNext",
        "*": "toggleSelection",
        "<tab>": "actionMenu",
        "h": "collapse",
        "gl": "expandRecursive",
        "gh": "collapseRecursive",
        "<cr>": "open",
        "l": [
            "expandable?",
            "expand",
            "open:tab"
        ],
        "L": "open:vsplit",
        "o": [
            "expanded?",
            "collapse",
            "expand"
        ],
        "O": "open:tab",
        "J": "gotoParent",
        "n": "gotoParent",
        "yp": "copyFilepath",
        "yn": "copyFilename",
        "yy": "copyFile",
        "dd": "cutFile",
        "pp": "pasteFile",
        "dD": "deleteForever",
        "a": "addFile",
        "M": "addDirectory",
        "cw": "rename",
        ".": "toggleHidden",
        "zh": "toggleHidden",
        "R": "refresh",
        "?": "help",
        "q": "quit",
        "X": "systemExecute",
        "gd": "listDrive",
        "f": "search",
        "F": "searchRecursive",
        "B": "gotoSource:file",
        "b": "gotoSource:buffer",
        "[[": "sourcePrev",
        "]]": "sourceNext",
        "[d": "diafnosticPrev",
        "]d": "diafnisticNext",
        "[c": "gitPrev",
        "]C": "gitNext",
        "<<": "gitStage",
        ">>": "gitUnstage"
